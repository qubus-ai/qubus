{
  "name": "qubus",
  "version": "0.0.0",
  "main": "./dist/backend/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-angular": "ng build --base-href ./",
    "build-backend": "tsc -p ./backend/tsconfig.json",
    "build-backend-run": "tsc -p ./backend/tsconfig.json && npm run qubus-dev",
    "build-release": "npm run build-angular && npm run build-backend && npm run utils-git",
    "build-prod": "npm run build-angular && npm run build-backend && npm run utils-git && npm run qubus",
    "build-dev": "npm run build-angular && npm run build-backend && npm run utils-git && npm run qubus-dev",
    "build-remote": "npm run build-backend && npm run utils-git && cross-env NODE_ENV=dev npm run start",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "qubus": "electron .",
    "qubus-dev": "electron . --dev",
    "qubus-remote": "electron . --dev --remote",
    "packager-osx": "npm run build-release && electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/qubus_logo.icns --prune=true --out=release-builds && npm run zip-osx-release",
    "packager-windows": "npm run build-release && electron-zip-packager . qubus --overwrite --asar=true --platform=win32 --arch=x64 --icon=assets/icons/win/qubus_logo.ico --prune=true --out=release-builds --version-string.CompanyName=Qubus --version-string.FileDescription=Qubus --version-string.ProductName=\"Qubus\"",
    "packager-linux": "npm run build-release && electron-packager . qubus --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds && npm run tar-release",
    "tar-release": "tar -zcvf ./release-builds/qubus-linux-x64.tar.gz -C ./release-builds ./qubus-linux-x64",
    "zip-osx-release": "cd ./release-builds && zip -r ./qubus-darwin-x64.zip ./qubus-darwin-x64",
    "utils-git": "node utils-git.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/cdk": "^7.3.4",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/http": "^7.1.0",
    "@angular/material": "^7.3.4",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@types/exif": "^0.6.0",
    "@types/geojson": "^7946.0.6",
    "@types/gm": "^1.18.2",
    "@types/ol": "^5.3.0",
    "core-js": "^2.5.4",
    "exif": "^0.6.0",
    "geojson": "^0.5.0",
    "gm": "^1.23.1",
    "jimp": "0.6.4",
    "ngx-electron": "^2.1.1",
    "ol": "^5.3.3",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "~7.1.0",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "cross-env": "^5.2.0",
    "electron": "^9.1.0",
    "electron-packager": "^13.1.1",
    "electron-zip-packager": "^4.0.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
